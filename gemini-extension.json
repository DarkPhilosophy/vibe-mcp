{
  "name": "vibe",
  "version": "0.2.1",
  "repository": "https://github.com/DarkPhilosophy/vibe-mcp",
  "description": "Vibe CLI programmed mode extension",
  "mcpServers": {
    "vibe": {
      "command": "node",
      "args": [
        "${extensionPath}/index.js"
      ],
      "env": {
        "VIBE_MCP_MONTHLY_LIMIT_EUR": "150"
      },
      "cwd": "${extensionPath}"
    }
  }
}
